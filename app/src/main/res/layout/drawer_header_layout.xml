<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#C8D2FA">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieAnimationView2422"
        android:layout_width="125dp"
        android:layout_height="91dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.29"
        app:lottie_autoPlay="true"
        app:lottie_fileName="splashanimation.json"
        app:lottie_loop="true" />


    <TextView
        android:id="@+id/textView11"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:fontFamily="@font/cagliostro"
        android:text="NimbusOne"
        android:textAlignment="textStart"
        android:textColor="#4565A4"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/lottieAnimationView2422"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lottieAnimationView2422"
        app:layout_constraintTop_toTopOf="@+id/lottieAnimationView2422" />


</androidx.constraintlayout.widget.ConstraintLayout>